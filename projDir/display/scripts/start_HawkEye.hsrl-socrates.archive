#! /bin/csh

rehash

setenv PROJ_DIR /opt/local/hsrl/projDir
cd $PROJ_DIR/display/params

if ($#argv < 6) then
  /opt/local/bin/HawkEye -params HawkEye.hsrl-socrates.archive -start_time "2018 01 02 20 35 00" >& /dev/null &
else
  /opt/local/bin/HawkEye -params HawkEye.hsrl-socrates.archive -start_time "$1 $2 $3 $4 $5 $6" >& /dev/null &
endif

